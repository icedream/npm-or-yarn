{
  "presets": [
    ["@babel/preset-env", {
      "targets": {
        "node": "current"
      }
    }],
    "@babel/preset-flow"
  ],
  "plugins": [
    "@babel/plugin-proposal-object-rest-spread",
    ["babel-plugin-shebang", {
      "force": true,
      "replacement": "#!/usr/bin/env node"
    }]
  ]
}